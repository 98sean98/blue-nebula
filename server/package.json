{
  "name": "server",
  "version": "0.0.1",
  "description": "server to support the mobile app",
  "scripts": {
    "start": "node dist",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --watch package.json src",
    "build": "npm run prisma:generate && tsc && tsc-alias",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --fix --ext .ts",
    "prisma:migrate:dev": "npx prisma migrate dev --name migration",
    "prisma:migrate:prod": "npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "precommit": "tsc --noEmit && lint-staged && npx prisma format"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "14.x"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@types/graphql-fields": "^1.3.3",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.31",
    "@types/pg": "^7.14.10",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "apollo-server-testing": "^2.21.0",
    "apollo-server-types": "^0.6.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.21.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-module-resolver": "^0.16.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prisma": "2.23.0",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.2.6",
    "tsconfig-paths": "^3.9.0",
    "typegraphql-prisma": "0.14.3",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@graphql-tools/schema": "^7.1.3",
    "@prisma/client": "2.23.0",
    "apollo-server": "^2.25.3",
    "axios": "^0.21.2",
    "body-parser": "^1.19.0",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "^6.0.4",
    "graphql-scalars": "^1.9.3",
    "graphql-shield": "^7.5.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "universal-cookie": "^4.0.4"
  }
}
