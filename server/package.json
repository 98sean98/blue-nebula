{
  "name": "scraper-server",
  "version": "0.0.1",
  "description": "server to support the mobile app",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --watch package.json src",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --fix --ext .ts",
    "prisma:migrate": "npx prisma migrate dev --name migration --preview-feature",
    "precommit": "tsc --noEmit && lint-staged"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.21.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-module-resolver": "^0.16.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prisma": "^2.18.0",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@graphql-tools/schema": "^7.1.3",
    "@prisma/client": "^2.18.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0"
  }
}
